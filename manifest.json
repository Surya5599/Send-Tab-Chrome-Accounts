  {
    "name": "Send Tab: Chrome Accounts",
    "version": "1.0.0",
    "description": "Send a tab to a different chrome window account.",
    "permissions": ["declarativeContent", "tabs", "identity", "identity.email","contextMenus","storage"],
    "background": {
      "scripts": ["jquery.js", "socket.io.js", "background.js"]
    },
    "page_action": {
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }
